	script.
		TS.onAppReady(UIinit)
		function UIinit() {
			console.log('x')
			TS.loadComp($('#comps'),'/_webComp/x-vid.html', UICompInit)
		}

		function UICompInit() {//this does not execute yet
			var el = customElements.get('x-vid')
			if(!el) // if it is not defined, define:
				customElements.define('x-vid', VidEl)// loaded w/ template
		}

